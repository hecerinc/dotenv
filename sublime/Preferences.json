{
	"color_scheme": "Packages/Theme - One Dark/One Dark.tmTheme",
	"folder_exclude_patterns": [
		".svn",
		".git",
		".hg",
		"CVS",
		"node_modules",
		".pytest_cache",
		"__pycache__"
